{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- vite",
    "build": "dotenvx run -- vite build",
    "preview": "dotenvx run -- vite preview",
    "lint": "oxlint -c ../.oxlintrc.json --import-plugin --react-plugin --jsx-a11y-plugin --tsconfig ./tsconfig.json ./src",
    "lint:fix": "oxlint -c ../.oxlintrc.json --import-plugin --react-plugin --jsx-a11y-plugin --tsconfig ./tsconfig.json --fix ./src",
    "format": "oxfmt -c ../.oxfmtrc.json --write ./src",
    "format:check": "oxfmt -c ../.oxfmtrc.json --check ./src"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19",
    "@google/genai": "^1.34.0",
    "lucide-react": "^0.562.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "5.1.2",
    "autoprefixer": "10.4.23",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "vite": "7.3.0"
  }

}
